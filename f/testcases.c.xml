<cdb>
 <c label="laminar_cavity_re=0100">
  <comment>
   <p>Shear-driven flow in a square cavity:</p>
   <p>Re=100, converges with pure Newton linearization.</p>
  </comment>
  <setup
   dimensions="2"
   restart="false"
   file_input="testcases/laminar_cavity_001kn.plt"
  />
  <terms
   density="1."
   kviscosity="1.e-2"
   prandtl="0.7"
   vardensity_value="1.e-3"
   gx="0."
   gy="-9.81"
   scalarscheme="PSI"
   diffusion="true"
   turbulence="NONE"
   temperature="false"
   vardensity="false"
   buoyancy="false"
  />
  <solver_nonlinear
   maxiterations="100"
   convergence_level="-10."
   convergence_variable="p"
   relax_puvw_type="linear"
   relax_puvw_value="1."
   relax_t_type="linear"
   relax_t_value="1."
   relax_ke_type="linear"
   relax_ke_value="1."
   method="Newton"
   newton_eps="1.e-8"
   newton_switch="-1."
   couple_t="false"
   couple_ke="false"
  />
  <system_coupled
   type="ls_wsmp"
   mtype="vbr"
   output="none"
   kspace="25"
   max_iter="100"
   overlap="none"
   precond="dom_decomp"
   subdomain_solve="bilu"
   solver="gmres"
   tol="1.0e-4"
  />
  <vars>
   <v label="p" init="0.01"/>
   <v label="u" init="0.001"/>
   <v label="v" init="0.001"/>
  </vars>
  <bcs>
   <bc zone="Boundary1" label="right" type="wall"/>
   <bc zone="Boundary2" label="left" type="wall"/>
   <bc zone="Boundary3" label="bottom" type="wall"/>
   <bc zone="Boundary4" label="top" type="wall" option="m" values="1. 0."/>
  </bcs>
 </c>
