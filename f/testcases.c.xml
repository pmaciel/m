 <c label="bfstep_supersaturation">
  <comment/>
  <setup
   dimensions="2"
   restart="false"
   file_input="testcases/bfstep_supersaturation_016kn.plt"
   file_output="bfstep_supersaturation_flow.plt"
   __restart="true"
   __file_input="bfstep_supersaturation_flow.plt"
   __file_output="bfstep_supersaturation_ec.plt"
  />
  <terms
   density="1000."
   kviscosity="0.801e-6"
   prandtl="0.7"
   vardensity_value="1.e-3"
   gx="0."
   gy="0."
   scalarscheme="PSI"
   diffusion="true"
   turbulence="NONE"
   temperature="false"
   vardensity="false"
   buoyancy="false"
  />
  <mitremassembler>
   <MITReM
    file="electrochemistry.ec.xml"
    label="Na2SO4_and_H2SO4_2009-10-13"
   />
   <ElementMatrixAssembler
    convectionScheme="N"
    diffusionScheme="Galerkin"
    migrationScheme="Galerkin"
    magneticScheme="Empty"
    homReactionScheme="Galerkin"
    electrostaticsScheme="Galerkin"
    timeScheme="Galerkin"
    elecReactionScheme="Pointwise"
    gasReactionScheme="Galerkin"
   />
   <ls type="ls_pardiso" />
   <iterinit value="1000" />
   <__iterinit value="0" />
   <linrelx value="1." />
   <surfacegasfraction min="0." max="1." />
   <bcs>
    <bc type="bulk"      label="inlet"      zone="inlet" />
    <bc type="electrode" label="WElectrode" zone="e0" metalpotential="-0.1" elecreaction="H2EvolutionFromH:H2EvolutionFromH2O:O2Evolution" gasreaction="H2ToGas" />
    <bc type="electrode" label="CElectrode" zone="e1" metalpotential=" 0.1" elecreaction="H2EvolutionFromH:H2EvolutionFromH2O:O2Evolution" />
    <bc type="electrode" label="GasWall"    zone="w0:w1:w2" gasreaction="H2ToGas" />
   </bcs>
  </mitremassembler>
  <solver_nonlinear
   maxiterations="200"
   convergence_level="-10."
   convergence_variable="p"
   relax_puvw_type="linear"
   relax_puvw_value="1."
   relax_t_type="linear"
   relax_t_value="1."
   relax_ke_type="linear"
   relax_ke_value="1."
   method="Newton"
   __method="__Newton"
   newton_eps="1.e-8"
   newton_switch="-4."
   couple_t="false"
   couple_ke="false"
  />
  <system_coupled type="ls_pardiso" />
  <vars>
   <v label="p" init="0."/>
   <v label="u" init="0.02"/>
   <v label="v" init="0.001"/>
  </vars>
  <bcs>
   <bc zone="Boundary1" label="inlet" type="fixv" option="p" values="0.02 0. 1."/>
   <bc zone="Boundary2" label="w0" type="wall"/>
   <bc zone="Boundary3" label="w1" type="wall"/>
   <bc zone="Boundary4" label="e0" type="wall"/>
   <bc zone="Boundary4" label="w2" type="wall"/>
   <bc zone="Boundary5" label="outlet" type="fixp" option="x" values="0."/>
   <bc zone="Boundary6" label="w3" type="wall"/>
   <bc zone="Boundary7" label="e1" type="wall"/>
   <bc zone="Boundary8" label="w4" type="wall"/>
  </bcs>
 </c>
