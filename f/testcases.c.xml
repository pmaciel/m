 <c label="turbulent_bfstep_ds_akn">
  <comment>
   <p>Turbulent backwards-facing step:</p>
   <p>Turbulent flow past a backward-facing step for the conditions examined by Driver and Seegmiller.</p>
  </comment>
  <setup
   dimensions="2"
   restart="false"
   file_input="testcases/turbulent_bfstep_ds_022kn.plt"
   file_inlet="testcases/turbulent_bfstep_ds_inlet_ke.dat"
   file_output="turbulent_bfstep_ds_akn.smurf"
  />
  <terms
   density="1."
   kviscosity="2.67e-5"
   prandtl="0.7"
   vardensity_value="1.e-3"
   gx="0."
   gy="0."
   scalarscheme="N"
   diffusion="true"
   turbulence="KENA"
   temperature="false"
   vardensity="false"
   buoyancy="false"
  />
  <solver_nonlinear
   maxiterations="1000"
   convergence_level="-12."
   convergence_variable="p"
   relax_puvw_type="linear"
   relax_puvw_value="1."
   relax_t_type="linear"
   relax_t_value="1."
   relax_ke_type="linear"
   relax_ke_value="1."
   method="Picard"
   newton_eps="1.e-8"
   newton_switch="-4."
   turb_iterinit="5"
   couple_t="false"
   couple_ke="true"
  />
  <system_coupled
   type="ls_aztec"
   mtype="vbr"
   output="none"
   kspace="50"
   max_iter="250"
   overlap="none"
   precond="dom_decomp"
   subdomain_solve="bilu"
   solver="gmres"
   tol="5.0e-2"
  />
  <system_turb
   type="ls_aztec"
   mtype="vbr"
   output="none"
   kspace="50"
   max_iter="100"
   overlap="none"
   precond="dom_decomp"
   subdomain_solve="ilu"
   solver="gmres"
   tol="1.0e-2"
  />
  <vars>
   <v label="p" init="0."/>
   <v label="u" init="0.750"/>
   <v label="v" init="0.001"/>
   <v label="k" init="0.05"/>
   <v label="e" init="5.00"/>
  </vars>
  <bcs>
   <bc zone="Boundary1" label="wall-u" type="wall"/>
   <bc zone="Boundary2" label="wall-s" type="wall"/>
   <bc zone="Boundary3" label="wall-d" type="wall"/>
   <bc zone="Boundary4" label="exit" type="fixp" option="x" values="0."/>
   <bc zone="Boundary5" label="top" type="wall"/>
   <bc zone="Boundary6" label="inlet" type="fixv" option="r" values="1. 1."/>
  </bcs>
 </c>
 <c label="turbulent_bfstep_ds_sst">
  <comment>
