 <c label="laminar_bfstep_re=0100">
  <comment>
   <p>Laminar backwards-facing step:</p>
   <p>Laminar flow past a backward-facing step.</p>
   <p>Re=100, based on inlet bulk velocity and twice inlet duct width. Converges with pure Newton linearization.</p>
  </comment>
  <setup
   dimensions="2"
   restart="false"
   file_input="testcases/laminar_bfstep_006kn.plt"
  />
  <terms
   density="1."
   kviscosity="2.128e-2"
   prandtl="0.7"
   gx="0."
   gy="0."
   diffusion="true"
   temperature="false"
   buoyancy="false"
   vardensity="false"
   vardensity_value="1.e-3"
   scalarscheme="PSI"
   turbulence="NONE"
  />
  <solver_nonlinear
   maxiterations="100"
   convergence_level="-12."
   convergence_variable="p"
   relax_puvw_type="linear"
   relax_puvw_value="1."
   relax_t_type="linear"
   relax_t_value="1."
   relax_ke_type="linear"
   relax_ke_value="1."
   method="Newton"
   newton_eps="1.e-8"
   newton_switch="-4."
   couple_t="false"
   couple_ke="false"
  />
  <system_coupled
   type="ls_aztec"
   mtype="vbr"
   output="none"
   kspace="25"
   max_iter="150"
   overlap="none"
   precond="dom_decomp"
   subdomain_solve="bilu"
   solver="gmres"
   tol="1.0e-4"
  />
  <vars>
   <v label="p" init="0."/>
   <v label="u" init="0.50"/>
   <v label="v" init="0.01"/>
  </vars>
  <bcs>
   <bc zone="Boundary1" label="wall-i" type="wall"/>
   <bc zone="Boundary2" label="wall-v" type="wall"/>
   <bc zone="Boundary3" label="wall-b" type="wall"/>
   <bc zone="Boundary4" label="outlet" type="fixp" option="x" values="0."/>
   <bc zone="Boundary5" label="wall-t" type="wall"/>
   <bc zone="Boundary6" label="inlet" type="fixv" option="p" values="1. 0. 1."/>
  </bcs>
 </c>
